{
  "name" : "Twitter",
  "description" : "Twitter Bot to auto follow",
  "version" : "1.0",
  "manifest_version" : 3,
  "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },

  "background" : {
       "service_worker" : "background.js"
   },

  "action" : {
       "default_popup" : "popup/popup.html"
   },
  "permissions": [
    "activeTab",
    "scripting",
    "notifications"
    ]

   
}
